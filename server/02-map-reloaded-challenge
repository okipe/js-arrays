function addNewAttr(array) {
  return array.map(item => ({
    ...item, 'taxes': Math.trunc (item.price * 0.19)
  }))
}

/* CONSOLE ***********************
Input:
[
  {
    name: "Product 1",
    price: 1000,
    stock: 10
  },
  {
    name: "Product 2",
    price: 2000,
    stock: 20
  }
]

Output:
[
  {
    name: "Product 1",
    price: 1000,
    stock: 10,
    taxes: 190
  },
  {
    name: "Product 2",
    price: 2000,
    stock: 20,
    taxes: 380
  }
]

*/